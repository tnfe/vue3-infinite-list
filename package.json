  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/compiler-sfc": "^3.2.31",
    "element-plus": "latest",
    "sass": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
