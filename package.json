{
  "name": "vue3-infinite-list",
  "version": "0.2.4",
  "author": "tnfe",
  "license": "MIT",
  "description": "An infinite scrolling list of vue3, of course vue2 can also be used.",
  "keywords": [
    "infinite-list",
    "infinite-scroll",
    "vue3"
  ],
  "private": false,
  "main": "dist/vue-infinite-list.umd.js",
  "exports": {
    ".": {
      "import": "./dist/vue-infinite-list.umd.js",
      "require": "./dist/vue-infinite-list.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tnfe/vue-infinite-list"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "docs": "vite build --config vite.doc.config.js",
